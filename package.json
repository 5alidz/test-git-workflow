{
  "name": "test-git-workflow",
  "version": "0.0.6",
  "main": "index.js",
  "author": "khaled <kzack-dev@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "release": "standard-version",
    "push": "git push --follow-tags origin master",
    "gh-release": "gh release create $(git tag --list | tail -n 1) -n $(changelog -t $(git tag --list | tail -n 1) -f -)"
  },
  "devDependencies": {
    "cz-conventional-changelog": "3.3.0",
    "standard-version": "^9.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
